{
    "name": "ireceipt",
    "version": "0.4.0",
    "description": "Generate Taiwan's electronic receipt including QR code, bar code, and XML for National Taxation Bureau",
    "main": "dist/js/ireceipt.min.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "run-p build:*",
        "build:js": "node_modules/.bin/rollup --environment NODE_ENV:production --config build/rollup.config.js --sourcemap",
        "build:css": "run-s css:*",
        "dev": "run-p dev:*",
        "dev:js": "node_modules/.bin/rollup --environment NODE_ENV:develop --config build/rollup.config.js --sourcemap --watch",
        "css:compile": "node_modules/.bin/node-sass --output-style expanded --source-map true --source-map-contents true --output dist/css src/scss",
        "css:minify": "node_modules/.bin/cleancss --level 1 --source-map --source-map-inline-sources --output dist/css/ireceipt.min.css src/scss/ireceipt.scss"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kf99916/iReceipt.git"
    },
    "keywords": [
        "electronic",
        "receipt",
        "taiwan",
        "xml",
        "taxation",
        "bureau"
    ],
    "author": {
        "name": "Zheng-Xiang Ke",
        "email": "kf99916@gmail.com",
        "url": "https://medium.com/@kf99916"
    },
    "contributors": [
        {
            "name": "Zheng-Xiang Ke",
            "email": "kf99916@gmail.com",
            "url": "https://medium.com/@kf99916"
        },
        {
            "name": "Sin-Fong Lyu",
            "email": "kingispeak@gmail.com"
        }
    ],
    "license": "MIT",
    "engineStrict": true,
    "engines": {
        "node": ">= 5.10.0"
    },
    "bugs": {
        "url": "https://github.com/kf99916/iReceipt/issues"
    },
    "homepage": "https://github.com/kf99916/iReceipt",
    "files": [
        "dist"
    ],
    "devDependencies": {
        "babel-preset-env": "~1.6",
        "babel-plugin-external-helpers": "~6.22",
        "clean-css-cli": "~4.1",
        "node-sass": "~4.7",
        "rollup": "~0.53",
        "rollup-plugin-babel": "~3.0",
        "rollup-plugin-babel-minify": "~3.1",
        "npm-run-all": "~4.1"
    },
    "dependencies": {
        "aes-js": "~3.1",
        "date-fns": "2.0.0-alpha.3",
        "jsbarcode": "~3.8",
        "qrcode": "~1.2",
        "xml2js": "~0.4"
    }
}
