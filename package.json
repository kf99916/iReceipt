{
    "name": "ireceipt",
    "version": "0.2.0",
    "description": "Generate Taiwan's electronic receipt including the layout and XML for National Taxation Bureau",
    "main": "dist/ireceipt.js",
    "style": "dist/css/receipt.css",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "css-compile": "node-sass src/scss/receipt.scss --output dist/css/",
        "build:dev": "babel src --out-dir dist --source-maps inline",
        "watch": "babel --watch src --out-dir dist --source-maps inline",
        "build": "babel src --out-dir dist --minified && npm run css-compile",
        "prepublish": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kf99916/iReceipt.git"
    },
    "keywords": [
        "electronic",
        "receipt",
        "taiwan",
        "xml",
        "taxation",
        "bureau"
    ],
    "author": "Zheng-Xiang Ke",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/kf99916/iReceipt/issues"
    },
    "homepage": "https://github.com/kf99916/iReceipt",
    "files": [
        "dist"
    ],
    "devDependencies": {
        "babel-cli": "~6.26",
        "babel-preset-env": "~1.6",
        "node-sass": "^4.7.2"
    },
    "dependencies": {
        "date-fns": "2.0.0-alpha.3",
        "xml2js": "~0.4"
    }
}
